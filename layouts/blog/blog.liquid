<div class="container">
	<h1 class="blog-title">{{ blog.title }}</h1>
	{% paginate articles by 9 %}
	<div class="row">
			{% for article in articles %}
			<div class="cell-4">
				{% include "article_preview" %}
			</div>
			{% endfor %}
		{% include "pagination" %}
	</div>
	{% endpaginate %}
</div>
